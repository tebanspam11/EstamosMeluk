{
  "expo": {
    "name": "PocketVet",
    "slug": "PocketVet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoPocketVet.jpg",
    "scheme": "pocketvet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.PocketVet"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/LogoPocketVet.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.anonymous.PocketVet"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/LogoPocketVet.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/LogoPocketVet.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicaci칩n necesita acceso a tus fotos para que puedas agregar im치genes de tus mascotas.",
          "cameraPermission": "La aplicaci칩n necesita acceso a tu c치mara para tomar fotos de tus mascotas."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/LogoPocketVet.jpg",
          "color": "#4A90E2",
          "sounds": [],
          "ios": {
            "requestPermissionsOnAppLaunch": true
          },
          "android": {
            "requestPermissionsOnAppLaunch": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
